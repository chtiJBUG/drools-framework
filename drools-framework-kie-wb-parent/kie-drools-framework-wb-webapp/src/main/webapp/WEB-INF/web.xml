<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">

  <distributable/>

  <listener>
    <listener-class>org.jboss.weld.environment.servlet.Listener</listener-class>
  </listener>
  <listener>
    <listener-class>org.jboss.resteasy.plugins.server.servlet.ResteasyBootstrap</listener-class>
  </listener>

  <filter>
    <filter-artifactId>request-capture</filter-artifactId>
    <filter-class>org.uberfire.ext.security.server.SecurityIntegrationFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-artifactId>request-capture</filter-artifactId>
    <url-pattern>*</url-pattern>
  </filter-mapping>

  <filter>
    <filter-artifactId>UberFire Security Headers Filter</filter-artifactId>
    <filter-class>org.uberfire.ext.security.server.SecureHeadersFilter</filter-class>
    <init-param>
      <param-artifactId>x-frame-options</param-artifactId>
      <param-value>SAMEORIGIN</param-value>
    </init-param>
    <init-param>
      <param-artifactId>x-xss-protection-enable</param-artifactId>
      <param-value>true</param-value>
    </init-param>
    <init-param>
      <param-artifactId>x-xss-protection-block</param-artifactId>
      <param-value>true</param-value>
    </init-param>
  </filter>

  <filter-mapping>
    <filter-artifactId>UberFire Security Headers Filter</filter-artifactId>
    <url-pattern>*</url-pattern>
  </filter-mapping>

  <!-- Basic Auth Filter for REST and Maven2 repo -->
  <filter>
    <filter-artifactId>HTTP Basic Auth Filter</filter-artifactId>
    <filter-class>org.uberfire.ext.security.server.BasicAuthSecurityFilter</filter-class>
    <init-param>
      <param-artifactId>realmName</param-artifactId>
      <param-value>KIE Workbench Realm</param-value>
    </init-param>
  </filter>

  <filter-mapping>
    <filter-artifactId>HTTP Basic Auth Filter</filter-artifactId>
    <url-pattern>/rest/*</url-pattern>
    <url-pattern>/maven2/*</url-pattern>
  </filter-mapping>

  <!-- RESTEasy configurations -->
  <context-param>
    <param-artifactId>resteasy.scan</param-artifactId>
    <param-value>true</param-value>
  </context-param>

  <context-param>
    <param-artifactId>resteasy.servlet.mapping.prefix</param-artifactId>
    <param-value>/rest</param-value>
  </context-param>

  <!-- Note, this configuration needs to be removed with AS7 deployement -->
  <context-param>
    <param-artifactId>resteasy.injector.factory</param-artifactId>
    <param-value>org.jboss.resteasy.cdi.CdiInjectorFactory</param-value>
  </context-param>

  <servlet>
    <servlet-artifactId>Resteasy</servlet-artifactId>
    <servlet-class>org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-artifactId>Resteasy</servlet-artifactId>
    <url-pattern>/rest/*</url-pattern>
  </servlet-mapping>

