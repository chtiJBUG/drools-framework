      <url-pattern>/org.kie.workbench.KIEWebapp/scorecardxls/file</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-artifactId>admin</role-artifactId>
      <role-artifactId>analyst</role-artifactId>
      <role-artifactId>developer</role-artifactId>
    </auth-constraint>
  </security-constraint>

